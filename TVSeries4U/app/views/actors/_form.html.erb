<%= form_for @actor do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name, class: 'form-control' %>
  
  <%= f.submit yield(:submit_text), class: "btn btn-primary" %>
<% end %>
